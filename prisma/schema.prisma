generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  Id             String @id @default(auto()) @map("_id") @db.ObjectId
  userName       String @unique
  accountNumber  String @unique
  emailAddress   String @unique
  identityNumber String @unique
}
